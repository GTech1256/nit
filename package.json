{
	"name": "nit-website",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"serve:front": "vue-cli-service serve",
		"build:front": "vue-cli-service build",
		"lint:front": "vue-cli-service lint",
		"dev:server": "cross-env NODE_ENV=development nodemon --exec babel-node server/index.js",
		"build:server": "cross-env NODE_ENV=production babel server --out-dir ./dist/server --copy-files",
		"serve:server": "cross-env NODE_ENV=development nodemon --exec babel-node ./server/index.js",
		"build": "cross-env NODE_ENV=production npm run build:front && npm run build:server",
		"serve": "cross-env NODE_ENV=production npm run build && npm run ss",
		"start": "npm run build && node ./dist/server/index.js",
		"start:dist": "node ./dist/server/index.js",
		"sf": "npm run serve:front",
		"ss": "npm run serve:server",
		"bf": "npm run build:front",
		"bs": "npm run build:server"
	},
	"dependencies": {
		"@babel/polyfill": "^7.2.5",
		"@babel/preset-es2015": "^7.0.0-beta.53",
		"@koa/cors": "^2.2.3",
		"axios": "^0.18.0",
		"bcryptjs": "^2.4.3",
		"cross-env": "^5.2.0",
		"dotenv": "^6.2.0",
		"jsonwebtoken": "^8.4.0",
		"koa": "^2.6.2",
		"koa-bodyparser": "^4.2.1",
		"koa-combine-routers": "^4.0.2",
		"koa-compose": "^4.1.0",
		"koa-cookie": "^1.0.0",
		"koa-helmet": "^4.0.0",
		"koa-json": "^2.0.2",
		"koa-jwt": "^3.5.1",
		"koa-logger": "^3.2.0",
		"koa-morgan": "^1.0.1",
		"koa-router": "^7.4.0",
		"koa-static": "^5.0.0",
		"koa2-history-api-fallback": "^0.0.5",
		"koa2-swagger-ui": "^2.11.9",
		"mongoose": "^5.4.1",
		"register-service-worker": "^1.5.2",
		"uuid": "^3.3.2",
		"vue": "^2.5.17",
		"vue-router": "^3.0.1",
