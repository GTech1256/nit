{
  "restartable": "rs",
  "ignore": [
    ".git",
    "node_modules",
    "src",
    "dist",
    "temporaryFiles",
    "logsData"
  ],
  "verbose": true,
  "execMap": {
    "js": "node"
  },
  "events": {
    "restart": "echo 'display notification \"App restarted due to:\n'$FILENAME'\" with title \"nodemon\"'"
  },
  "watch": [
    "./",
  ],
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "js json"
}
